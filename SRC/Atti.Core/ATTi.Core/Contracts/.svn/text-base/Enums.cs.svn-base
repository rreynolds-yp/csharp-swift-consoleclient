namespace ATTi.Core.Contracts
{
	

	/// <summary>
	/// Contract kinds -- ala' design by contract.
	/// </summary>
	public enum ContractType
	{
		/// <summary>
		/// The contract represents a precondition.
		/// </summary>
		Precondition,
		/// <summary>
		/// The contract represents a postcondition.
		/// </summary>
		Postcondition,
		/// <summary>
		/// The contract represents an invariant condition
		/// </summary>
		Invariant,
		/// <summary>
		/// The contract represents an intermediate state
		/// </summary>
		Intermediate,
	}

	
}